#!/bin/sh
#
# $FreeBSD: releng/10.3/etc/rc.d/ipropd_slave 272870 2014-10-09 23:47:07Z hrs $
#

# PROVIDE: ipropd_slave
# REQUIRE: kdc
# KEYWORD: shutdown

. /etc/rc.subr

name=ipropd_slave
rcvar=${name}_enable
required_files="$ipropd_slave_keytab"
start_precmd=${name}_start_precmd

ipropd_slave_start_precmd()
{

	if [ -z "$ipropd_slave_master" ]; then
		warn "\$ipropd_slave_master is empty."
		return 1
	fi
	command_args=" \
	    $command_args \
	    --keytab=\"$ipropd_slave_keytab\" \
	    --detach \
	    $ipropd_slave_master"
}

load_rc_config $name
run_rc_command "$1"
